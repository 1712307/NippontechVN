.enamel-contentsBlock--moisture .enamel-contentsBlock__image {background: url(images/thong-tin/dac-tinh-san-pham/image_moisture.jpg) no-repeat center center;background-size: cover;}
.enamel-contentsBlock--odor .enamel-contentsBlock__image {background: url(images/thong-tin/dac-tinh-san-pham/image_material.jpg) no-repeat center center;background-size: cover;}.enamel-contentsBlock--scratched .enamel-contentsBlock__image {background: url(images/thong-tin/dac-tinh-san-pham/image_scratched.jpg) no-repeat center center;background-size: cover;}
.enamel-contentsBlocks--column2 {display: flex;}.enamel-contentsBlocks--column2 .enamel-contentsBlock {width: 50%;display: block;}
.enamel-contentsBlock--material .enamel-contentsBlock__image {background: url(images/thong-tin/dac-tinh-san-pham/image_odor.jpg) no-repeat center center;background-size: cover;}.enamel-contentsBlocks--column2 .enamel-contentsBlock__image {width: 100%;}.enamel-contentsBlock--material .enamel-contentsBlock__titleWrap {background: #fafafa;}.enamel-contentsBlocks--column2 .enamel-contentsBlock__titleWrap {padding: 40px;width: 100%;}.enamel-contentsBlock__titleWrap {padding: 0 40px;box-sizing: border-box;width: 41.66667%;display: flex;align-items: center;justify-content: center;color: #747474;background: #eee;}
.enamel-contentsBlock--heat .enamel-contentsBlock__image {background: url(images/thong-tin/dac-tinh-san-pham/image_heat.jpg) no-repeat center center;background-size: cover;}.enamel-contentsBlock--heat .enamel-contentsBlock__titleWrap {background: #fff;}.enamel-contentsBlocks--column2 .enamel-contentsBlock__image {width: 100%;}.callback-pd{margin-top: 10px }
.title-section img{width: 100%;margin-bottom: 10px }.enamel-contentsBlock__title h3::after {content: '';height: 1px;position: absolute;width: 100%;bottom: -5px;left: 0;background: #747474;}.enamel-contentsBlock__title {text-align: justify;;}#icon-dactinh .item:hover {background: #9b9b9b54;}#icon-dactinh .item{padding: 5px 3px 0px }#icon-dactinh .item:after{height: 100% }@media (max-width: 769px){.enamel-contentsBlock__image {padding-top: 50%;width: 100%;height: auto;}.enamel-contentsBlock__titleWrap {padding: 20px 20px 35px;box-sizing: border-box;width: 100%;}.enamel-contentsBlocks--column2 .enamel-contentsBlock {width: 100%;}.enamel-contentsBlocks--column2 {display: block;}
.fz-4 {font-size: 1.8rem;}}@media only screen and (max-width:576px){img{width: 100% }.title-section h2{font-size: 21px !important;padding-top:0px}#icon-dactinh{display:none}.enamel-contentsBlocks{margin-top:-40px}.title-product::after {bottom: 5px;z-index: 2;top: inherit !important;}}
